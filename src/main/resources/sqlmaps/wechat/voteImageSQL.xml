<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xiaoshabao.wechat.dao.VoteImageDao">
	<select id="getImageList" resultType="com.xiaoshabao.wechat.entity.VoteImageEntity">
		select player_id,vote_id,type,image
		from wx_vote_image
		where player_id=#{playerId}
	</select>
	<insert id="insertImage">
	INSERT into wx_vote_image(vote_id,type,player_id,image)
	VALUES(#{voteId,jdbcType=NUMERIC},#{type,jdbcType=VARCHAR},#{playerId,jdbcType=NUMERIC},#{image,jdbcType=VARCHAR})
	</insert>
</mapper>
